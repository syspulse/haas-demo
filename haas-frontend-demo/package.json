  "scripts": {
    "setup":"yarn && touch src/common/config/deploys.json && yarn build-version",
    "start": "HTTPS=true && craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "build-version": "node deploy-version.js",
    "deploy": "yarn build-version && yarn build && yarn sync:s3 && yarn cf:invalidate",
